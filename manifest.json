{
  "name": "Shellix Editor",
  "short_name": "ShellixEdit",
  "start_url": "index.html",
  "display": "standalone",
  "background_color": "#1b1d22",
  "theme_color": "#1b1d22",
  "icons": [
    {
      "src": "icons/icon-192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "icons/icon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "file_handlers": [
    {
      "action": "index.html",
      "accept": {
        "text/plain": [
          ".txt",
          ".html",
          ".js",
          ".json",
          ".c",
          ".cpp",
          ".css",
          ".ts",
          ".php",
          ".ps1",
          ".sh"
        ]
      }
    }
  ]
}